      "source": [
        "numero = int(input(\"Digite um número inteiro: \"))\n",
        "\n",
        "fatorial = 1\n",
        "\n",
        "for i in range(1, numero + 1):\n",
        "    fatorial *= i\n",
        "\n",
        "print(\"O fatorial de {} é {}.\".format(numero, fatorial))\n"
